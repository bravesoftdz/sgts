/* Создание просмотра полевого журнала 1 напряженно-деформированного состояния */

CREATE OR REPLACE VIEW S_NDS_JOURNAL_FIELDS_1
AS 
SELECT CYCLE_ID,
       CYCLE_NUM,
       JOURNAL_FIELD_ID,
       JOURNAL_NUM,
       DATE_OBSERVATION,
       MEASURE_TYPE_ID,
       POINT_ID,
       POINT_NAME,
       COORDINATE_Z,
       CONVERTER_ID,
       CONVERTER_NAME,
       NOTE,
       TYPE_INSTRUMENT,
       VALUE_RESISTANCE_LINE,
       VALUE_RESISTANCE,
       VALUE_FREQUENCY,
       VALUE_PERIOD,
       VALUE_STATER_CARRIE,
       OBJECT_PATHS
  FROM S_NDS_JOURNAL_FIELDS_O1
UNION
SELECT CYCLE_ID,
       CYCLE_NUM,
       JOURNAL_FIELD_ID,
       JOURNAL_NUM,
       DATE_OBSERVATION,
       MEASURE_TYPE_ID,
       POINT_ID,
       POINT_NAME,
       COORDINATE_Z,
       CONVERTER_ID,
       CONVERTER_NAME,
       NOTE,
       TYPE_INSTRUMENT,
       VALUE_RESISTANCE_LINE,
       VALUE_RESISTANCE,
       VALUE_FREQUENCY,
       VALUE_PERIOD,
       VALUE_STATER_CARRIE,
       OBJECT_PATHS
  FROM S_NDS_JOURNAL_FIELDS_N1

--

/* Фиксация изменений */

COMMIT


