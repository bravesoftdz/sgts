unit SgtsInterfaceModulesIntf;

interface

uses SgtsModulesIntf, SgtsInterfaceIntf;

type

  ISgtsInterfaceModule=interface(ISgtsModule)
  ['{3EBB8FF2-4BE0-4A66-8BD5-4A8EEA6C6021}']
  { methods }
    procedure InitByInterface(AInterfaceIntf: ISgtsInterface);

  { properties }
    function _GetInterface: ISgtsInterface;

    property Iface: ISgtsInterface read _GetInterface;
  end;

  ISgtsInterfaceModules=interface(ISgtsModules)
  ['{D718F8D6-43E8-4307-BCAD-EAE3F394C4B5}']
  end;

implementation

end.
